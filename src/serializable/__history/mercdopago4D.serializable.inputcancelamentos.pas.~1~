unit mercdopago4D.serializable.cancelamentos;

interface

type
  TInputCancelamentos = class
  private
    FStatus: String;
  public
    property Status: String read FStatus write FStatus;
    class function New: TInputCancelamentos;
  end;

implementation

{ TCancelamentos }

class function TInputCancelamentos.New: TInputCancelamentos;
begin
  Result := Self.Create;
end;

end.
