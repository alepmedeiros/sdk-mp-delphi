unit mercdopago4D.serializable.additionalinfo;

interface

uses
  System.Generics.Collections,
  GBJSON.Attributes;

type
  TAdditionalInfo = class
  private
    FItems: TObjectList<Items>;
  public
    [JSONProp(items)]
    property Items: TObjectList<Items> read FItems write FItems;
    class function New: TAdditionalInfo;
  end;

implementation

{ TAdditionalInfo }

class function TAdditionalInfo.New: TAdditionalInfo;
begin
  Result := Self.Create;
end;

end.
