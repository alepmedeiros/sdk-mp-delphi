unit mercdopago4D.serializable.impl.devices;

interface

uses
  mercdopago4D.serializable.interfaces,
  mercdopago4D.serializable.devices;

type
  TDevices = class(TInterfacedObject, iDevices)
  private
    FDevices: TDevicesSerialize;
    FSerialize: iSerializable<TDevicesSerialize>;

    constructor Create;
    destructor Destroy; override;
  public
    class function New: iDevices;
    function Id: String;
    function OperatingMode: string;
  end;

implementation

uses
  mercdopago4D.serializable;


constructor TDevices.Create;
begin
  FDevices:= TDevicesSerialize.New;
  FSerialize:= TSerializable<TDevicesSerialize>.New(FDevices);
end;

destructor TDevices.Destroy;
begin

  inherited;
end;

function TDevices.Id: String;
begin

end;

class function TDevices.New: iDevices;
begin
  Result := Self.Create;
end;

function TDevices.OperatingMode: string;
begin

end;

end.
